<launch>
  <!-- Launch Gazebo  -->
  <include file="$(find Cart)/launch/Cart_world.launch" />   


  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find Cart)/config/Cart_control.yaml" command="load"/>


  <!-- load the controllers -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
	output="screen" ns="/Cart" args="joint_state_controller
					  C_A1_L0_Pos
                                          C_A1_L1_Pos
                                          C_A1_L2_Pos
C_A1_L3_Pos
C_A1_L4_Pos
C_A2_L0_Pos
C_A2_L1_Pos
C_A2_L2_Pos
C_A2_L3_Pos
C_A3_L0_Pos
C_A3_L1_Pos
C_A3_L2_Pos
C_A3_L3_Pos
C_A3_L4_Pos
C_A4_L0_Pos
C_A4_L1_Pos
C_A4_L2_Pos
C_A4_L3_Pos
C_A4_L4_Pos"/>


  <!-- convert joint states to TF transforms for rviz, etc -->
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"
	respawn="false" output="screen">
    <remap from="/joint_states" to="/Cart/joint_states" />
  </node>

</launch>
